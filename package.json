  "private": true,
  "homepage": "https://sokomishalov.github.io/about-me",
  "dependencies": {
    "@ant-design/icons": "^5.2.5",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@octokit/graphql": "^7.0.1",
    "antd": "^5.8.4",
    "asciidoctor": "^3.0.2",
    "github-colors": "^2.2.20",
    "lodash": "^4.17.21",
    "luxon": "^3.4.2",
    "react": "^18.2.0",
    "react-awesome-reveal": "^4.2.7",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "react-typist-component": "^1.0.5",
    "rehype-raw": "^7.0.0"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.13.8",
    "customize-cra": "^1.0.0",
    "gh-pages": "^6.0.0",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.1",
    "source-map-explorer": "^2.5.3"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "watch-css": "node-sass src/ -o src/ --watch --recursive",
    "build-css": "node-sass src/ -o src/",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-app-rewired start",
    "build": "npm run build-css && react-app-rewired build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
