{
  "name": "about-me",
  "version": "0.0.1",
  "private": true,
  "homepage": "https://sokomishalov.github.io/about-me",
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "antd": "^5.21.6",
    "github-colors": "^2.2.20",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "react": "^18.3.1",
    "react-awesome-reveal": "^4.2.14",
    "react-dom": "^18.3.1",
    "react-typist-component": "^1.1.1"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.13.8",
    "customize-cra": "^1.0.0",
    "gh-pages": "^6.2.0",
    "sass": "^1.80.6",
    "npm-run-all": "^4.1.5",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.1",
    "source-map-explorer": "^2.5.3"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "watch-css": "sass --watch --update --style=expanded src:src",
    "build-css": "sass --no-source-map --style=compressed src:src",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-app-rewired start",
    "build": "npm run build-css && react-app-rewired build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
