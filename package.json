{
  "name": "about-me",
  "version": "0.0.1",
  "private": true,
  "homepage": "https://sokomishalov.github.io/about-me",
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@octokit/graphql": "^4.6.1",
    "antd": "^4.15.1",
    "asciidoctor": "^2.2.4",
    "github-colors": "^2.2.19",
    "lodash": "^4.17.21",
    "luxon": "^1.26.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-markdown": "^5.0.3",
    "react-reveal": "^1.2.2",
    "react-typist": "^2.0.5"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.13.3",
    "customize-cra": "^1.0.0",
    "gh-pages": "^3.1.0",
    "less": "^4.1.1",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "react-app-rewired": "^2.1.8",
    "react-scripts": "^4.0.3",
    "source-map-explorer": "^2.5.2"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "watch-css": "npm run build-less && node-sass-chokidar src/ -o src/ --watch --recursive",
    "build-css": "npm run build-less && node-sass-chokidar src/ -o src/",
    "build-less": "lessc --js src/styles/antd.less src/styles/antd.css",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-app-rewired start",
    "build": "npm run build-css && react-app-rewired build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
