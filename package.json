{
  "name": "about-me",
  "version": "0.0.1",
  "private": true,
  "homepage": "http://sokomishalov.github.io/about-me",
  "dependencies": {
    "antd": "^3.24.2",
    "antd-mobile": "^2.3.1",
    "lodash": "~4.17.15",
    "moment": "~2.24.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-reveal": "^1.2.2",
    "react-scripts": "3.2.0",
    "react-typist": "^2.0.5"
  },
  "devDependencies": {
    "babel-plugin-import": "~1.12.2",
    "customize-cra": "~0.8.0",
    "gh-pages": "^2.1.1",
    "less": "^3.10.3",
    "node-sass-chokidar": "1.3.5",
    "npm-run-all": "^4.1.5",
    "react-app-rewired": "~2.1.4",
    "react-scripts": "~3.2.0",
    "source-map-explorer": "~2.1.0",
    "webpack-ant-icon-loader": "~1.0.8"
  },
  "scripts": {
    "watch-css": "npm run build-less && node-sass-chokidar src/ -o src/ --watch --recursive",
    "build-css": "npm run build-less && node-sass-chokidar src/ -o src/",
    "build-less": "lessc --js src/styles/antd.less src/styles/antd.css",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-app-rewired start",
    "build": "npm run build-css && react-app-rewired build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
